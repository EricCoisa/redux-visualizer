{
  "name": "redux-visualizer",
  "version": "1.0.3",
  "description": "Redux Visualizer is a modal component that allows you to inspect, expand, and edit Redux states in real-time. It displays your state as an interactive tree structure and supports editing values of any type",
  "private": false,
  "main": "dist/redux-visualizer.umd.js",
  "module": "dist/redux-visualizer.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "redux-visualizer.css",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "vite build && tsc --project tsconfig.types.json",
    "build:app": "vite build --config vite.app.config.ts --outDir app-build",
    "dev": "vite",
    "preview": "vite preview"
  },
   "peerDependencies": {
    "@reduxjs/toolkit": "^2.9.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-redux": "^9.2.0",
    "redux": "^4.2.0 || ^5.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "vite-plugin-css-injected-by-js": "^3.5.2"
  },
  "dependencies": {
    "vite-plugin-dts": "^4.5.4"
  }
}
